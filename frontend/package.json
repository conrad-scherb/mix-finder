{
  "name": "mix-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@frasermcc/log": "^2.1.2",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "1001-tracklists-scraper": "^1.0.9",
    "perf_hooks": "^0.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm-run-all build:css --parallel watch:css start:react",
    "start:react": "react-scripts start",
    "prebuild": "cross-env NODE_ENV=production run-s build:css",
    "build": "react-scripts build",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:css": "chokidar \"./tailwind.config.js\" \"./src/styles/tailwind.css\" -c \"npm run build:css\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2"
  }
}
